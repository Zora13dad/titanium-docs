ameters":[{"name":"label","summary":"<p>The label to track the timer by</p>\n","type":"String","optional":true,"filename":"Global.Console.timeLog-method.label-param"},{"name":"data","summary":"<p>Extra log data to be provided when logging, can be a single argument or any number of arguments.</p>\n","type":"Object","optional":true,"filename":"Global.Console.timeLog-method.data-param"}],"returns":{"type":"void"}},{"name":"trace","summary":"<p>Log a message at the <code>trace</code> level.</p>\n","description":"<p>The message to log can either be a single argument, or any number\nof arguments, which will be converted to strings and then concatenated\ntogether with a space character.</p>\n","filename":"Global.Console.trace-method","platforms":[{"pretty_name":"Android","since":"9.1.0","name":"android"},{"pretty_name":"iPhone","since":"9.1.0","name":"iphone"},{"pretty_name":"iPad","since":"9.1.0","name":"ipad"},{"pretty_name":"Windows","since":"9.1.0","name":"windowsphone"}],"parameters":[{"name":"message","summary":"<p>The message(s) to log.</p>\n","type":"Object","optional":false,"filename":"Global.Console.trace-method.message-param"}],"returns":{"type":"void"}},{"name":"warn","summary":"<p>Log a message at the <code>warn</code> level.</p>\n","description":"<p>The message to log can either be a single argument, or any number\nof arguments, which will be converted to strings and then concatenated\ntogether with a space character.</p>\n","filename":"Global.Console.warn-method","platforms":[{"pretty_name":"Android","since":"2.1.0","name":"android"},{"pretty_name":"iPhone","since":"2.1.0","name":"iphone"},{"pretty_name":"iPad","since":"2.1.0","name":"ipad"},{"pretty_name":"Windows","since":"4.1.0","name":"windowsphone"}],"parameters":[{"name":"message","summary":"<p>The message(s) to log.</p>\n","type":"Object","optional":false,"filename":"Global.Console.warn-method.message-param"}],"returns":{"type":"void"}}],"properties":[],"events":[],"constants":[]}